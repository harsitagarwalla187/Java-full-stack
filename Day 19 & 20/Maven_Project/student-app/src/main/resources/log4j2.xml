<?xml version="1.0" encoding="UTF-8" ?>
<Configuration status="OFF">
    <Appenders>
        <File name="FileLogger" fileName="logs/log.txt" append="false">
            <PatternLayout>
                <Pattern>%d{yyyy-MM-dd HH:mm:ss} [%t] %-5level %c{1} - %msg%n</Pattern>
            </PatternLayout>
        </File>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="FileLogger" />
        </Root>
    </Loggers>
</Configuration>
